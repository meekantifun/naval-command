    "dotenv": "^17.2.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "express-session": "^1.18.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
