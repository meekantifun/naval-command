{
  "name": "naval-command-web",
  "version": "1.0.0",
  "description": "Interactive web interface for Naval Command Discord bot",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "client": "cd client && npm start",
